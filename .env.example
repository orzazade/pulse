# Convex deployment URL
# Get this from running `npx convex dev` in the project root
EXPO_PUBLIC_CONVEX_URL=

# Convex deployment name (for reference)
CONVEX_DEPLOYMENT=

# Clerk Authentication
# Get this from Clerk Dashboard → API Keys → Publishable key
EXPO_PUBLIC_CLERK_PUBLISHABLE_KEY=

# Clerk Issuer URL for Convex authentication
# Get this from Clerk Dashboard → JWT Templates → Convex template → Issuer URL
CLERK_ISSUER_URL=
